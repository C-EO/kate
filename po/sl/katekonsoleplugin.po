msgid "S&ynchronize Terminal with Current Document"
msgstr "Uskladi terminal s trenutnim dokumentom"

#: kateconsole.cpp:152
#, kde-format
msgctxt "@action"
msgid "Run Current Document"
msgstr "Zaženi trenutni dokument"

#: kateconsole.cpp:157 kateconsole.cpp:521
#, kde-format
msgctxt "@action"
msgid "S&how Terminal Panel"
msgstr "Prikaži ploščo terminala"

#: kateconsole.cpp:163
#, kde-format
msgctxt "@action"
msgid "&Focus Terminal Panel"
msgstr "Plošča terminala v žarišču"

#: kateconsole.cpp:169
