#, kde-format
msgid "You do not have enough karma to access a shell or terminal emulation"
msgstr ""
"Vaše karma není dostatečně silná pro přístup k příkazovému procesoru nebo "
"terminálu"

#: kateconsole.cpp:102 kateconsole.cpp:132 kateconsole.cpp:608
#, kde-format
msgid "Terminal"
msgstr "Terminál"

#: kateconsole.cpp:141
#, kde-format
msgctxt "@action"
msgid "&Pipe to Terminal"
msgstr "&Poslat rourou na terminál"

#: kateconsole.cpp:145
#, kde-format
msgctxt "@action"
msgid "S&ynchronize Terminal with Current Document"
msgstr "S&ynchronizovat terminál se současným dokumentem"

#: kateconsole.cpp:149
#, kde-format
msgctxt "@action"
msgid "Run Current Document"
msgstr "Spustit současný dokument"

